{
  "title": "Literal",
  "name": "literal",
  "description": "Render strings, templates, and files as JS Literals.",
  "version": "0.2.1",
  "author": {
    "name": "stephband",
    "url": "http://stephen.band",
    "twitter": "@stephband"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stephband/literal.git"
  },
  "bugs": {
    "url": "https://github.com/stephband/literal/issues/"
  },
  "devDependencies": {
    "clean-css": "^4.2.3",
    "esbuild": "^0.8.27",
    "findit": "^2.0.0",
    "madge": "^3.12.0",
    "sass": "^1.30.0"
  },
  "homepage": "https://stephen.band/literal",
  "docs": "https://stephen.band/literal",
  "type": "module",
  "module": "module.js",
  "scripts": {
    "build": "node --experimental-json-modules ../fn/build-module.js",
    "literal": "node --experimental-json-modules --no-warnings ./index.js ./ ./ debug",
    "watch": "node ./bolt/literal/build-watch.js"
  },
  "favicons": [],
  "literal": {
    "exclude": [
      ".git",
      "bolt",
      "dom",
      "fn",
      "node_modules",
